{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "BaseAddress": "https://localhost:8101",
  "Server": {
    "Url": "https://localhost:8001",
    "IsStatic": false
  },
  "IdentityProvider": {
    "Enable": true,
    "Endpoint": "https://localhost:8001/_configuration/AcBlog.Client.WebAssembly",
    "RemoteRegisterPath": "https://localhost:8001/Identity/Account/Register",
    "RemoteProfilePath": "https://localhost:8001/Identity/Account/Manage"
  }
}
